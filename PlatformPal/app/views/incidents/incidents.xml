<TabViewItem title="&#xf188; Incidents" class="fa-6">
  <TabViewItem.view>
        <ListView items="{{ incidentItems }}" itemTap="incidentsItemTap" row="1" separatorColor="#fff" class="notify-list-view">
          <ListView.itemTemplate>
            <GridLayout rows="20, auto" class="notify-view-item">
            	<Label text="{{'&#xf1e6;' + date }}" class="listItem" row= "0"/>
            	<Label text="{{ name }}" class="listItem" row = "1"/>
            </GridLayout>
          </ListView.itemTemplate>
        </ListView>
  </TabViewItem.view>
</TabViewItem>